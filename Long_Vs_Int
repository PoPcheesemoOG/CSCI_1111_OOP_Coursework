    long startTime = System.nanoTime();
		int x = 0;
		for(int i = 0; i < 2000000000; i++) {
			x++;
		}
		System.out.println("Time to compute " + (System.nanoTime() - startTime));
		
		startTime = System.nanoTime();
		long y = 0;
		for(int i = 0; i < 2000000000; i++) {
			y++;
		}
		System.out.println("Time to compute " + (System.nanoTime() - startTime));
